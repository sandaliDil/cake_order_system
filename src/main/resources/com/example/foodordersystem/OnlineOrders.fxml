<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.foodordersystem.controller.OnlineOrderController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="orderTable">
        <columns>
            <TableColumn fx:id="idColumn" text="Order ID"/>
            <TableColumn fx:id="optionColumn" text="Option"/>
            <TableColumn fx:id="dateColumn" text="Order Date"/>
            <TableColumn fx:id="actionColumn" text="Actions" />

        </columns>
    </TableView>

</AnchorPane>
